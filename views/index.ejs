<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <a href="/new">Add new message</a>
    <p>
      <% messages.forEach(message => { %>
        <p><%= message.user %>: <%= message.message %></p>
      <% }); %>
    </p>
  </body>
</html>
